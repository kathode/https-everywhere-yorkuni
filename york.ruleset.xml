<rulesetlibrary>
<ruleset name="YorkUni">
  <target host="york.ac.uk" />		
  <target host="*.york.ac.uk" />

  <rule from="^https://cs\.york\.ac\.uk/" to="https://www.cs.york.ac.uk/"/>
  <rule from="^http://cs\.york\.ac\.uk/" to="https://www.cs.york.ac.uk/"/>
  <rule from="^http://([^/:@]*)\.york\.ac\.uk/" to="https://$1.york.ac.uk/"/>
</ruleset>

<ruleset name="YorkSpace">
  <target host="*.yorkspace.net" />

  <rule from="^http://([^/:@]*)\.yorkspace\.net" to="https://$1.yorkspace.net/"/>
</ruleset>

<ruleset name="YUSU">
	<target host="*.yusu.org" />
	<rule from="^http://([^/:@]*)\.yusu\.org" to="https://$1.yusu.org/"/>
</ruleset>
</rulesetlibrary>
